            </Pane>
         </bottom>
         <left>
            <Button fx:id="button3" mnemonicParsing="false" onAction="#onfullscreen1" onSwipeLeft="#onGetLastImage" prefHeight="780.0" prefWidth="980.0" style="-fx-background-color: TRANSPARENT;" textFill="#a13030" BorderPane.alignment="CENTER">
               <graphic>
                  <ImageView fx:id="image3" fitHeight="262.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@/images/arrowleft.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </left>
         <right>
            <Button fx:id="button4" mnemonicParsing="false" onAction="#onfullscreen2" onSwipeRight="#onGetNextImage" prefHeight="780.0" prefWidth="948.0" style="-fx-background-color: TRANSPARENT;" BorderPane.alignment="CENTER">
               <graphic>
                  <ImageView fx:id="image4" fitHeight="150.0" fitWidth="274.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@/images/arrowright.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </right>
      </BorderPane>
   </children>
</AnchorPane>
