          <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
          <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" percentWidth="50.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints percentHeight="100.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="login" mnemonicParsing="false" onAction="#switchToLogin" text="Anmelden" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="rightbutton" mnemonicParsing="false" onAction="#switchToFilter" GridPane.columnIndex="2" />
      <Button fx:id="leftbutton" mnemonicParsing="false" onAction="#switchToAllImages" />
   </children>
</GridPane>
